<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.qidewang.dao.StudentDao">

    <insert id="addStudent" parameterType="cn.qidewang.doMain.Student">
        INSERT INTO student(name,age,sex,phone_number,score,update_time) VALUES(#{name},#{age},#{sex},#{number},#{score},now())
    </insert>

    <select id="getAllStudent" resultType="java.util.Map">
        SELECT * FROM student;
    </select>
</mapper>